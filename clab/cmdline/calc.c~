#include<stdio.h>
#include<stdlib.h>
int main(int argc, char *argv[])


{
int op;


op = *argv[2];

    switch (op)
    {
    case '+':
        
        printf("%d \n",atoi(argv[1])+atoi(argv[3]));
        break;
    case '-':
        
        printf("%d \n",atoi(argv[1])-atoi(argv[3]));
        break;        
    case '*':
        
        printf("%d \n",atoi(argv[1])*atoi(argv[3]));
        break;
       
    case '/':        
        printf("%3.2f \n",atof(argv[1])/atof(argv[3]));
        break;        
    default:
        printf("Invalid Operator \n");
     }	 

return 0;


}
